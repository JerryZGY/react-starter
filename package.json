  "scripts": {
    "clean": "rimraf dist/*",
    "bns": "yarn build && clear && yarn start",
    "dev": "yarn client:dev",
    "start": "node dist/index.js",
    "build": "yarn clean && yarn server:build && yarn client:build",
    "server:build": "cross-env NODE_ENV=production webpack --config config/webpack/server/webpack.prod.js",
    "client:dev": "cross-env NODE_ENV=development node scripts/start.js",
    "client:build": "cross-env NODE_ENV=production webpack --config config/webpack/client/webpack.prod.js"
  },
